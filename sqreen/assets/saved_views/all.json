{
    "timerange": {
        "interval": 86400000
    },
    "query": "source:sqreen",
    "name": "Sqreen | All logs",
    "visible_facets": [
        "core_index",
        "tag_source",
        "core_host",
        "core_service",
        "core_status",
        "log_network.client.ip",
        "log_sqreen.payload.automation_plugin_humanized_id",
        "log_http.useragent_details.os.family",
        "log_http.useragent_details.browser.family",
        "log_http.url_details.path",
        "log_sqreen.actor.identifier.username",
        "log_userIdentity.session_name",
        "log_sqreen.application.name",
        "log_sqreen.payload.protection",
        "log_http.useragent_details.device.family",
        "log_error.kind",
        "log_network.client.geoip.city.name",
        "log_sqreen.actor.user.auth_id",
        "log_sqreen.context.request.user_agent",
        "log_eventSource",
        "log_sqreen.actor.identifiers.email",
        "log_sqreen.payload.severity",
        "log_sqreen.application.environment",
        "log_sqreen.context.request.path",
        "log_http.method",
        "log_usr.account_id",
        "log_sqreen.actor.ip.geo.country_code",
        "log_usr.email",
        "log_network.client.geoip.subdivision.name",
        "log_sqreen.payload.properties.success",
        "log_userIdentity.arn",
        "log_http.referer",
        "log_sqreen.payload.attack",
        "log_http.request_id",
        "log_sqreen.signal_name",
        "log_sqreen.actor.ip.address",
        "log_sqreen.payload.name",
        "log_sqreen.payload.ruleset",
        "log_sqreen.context.response.blocked",
        "log_evt.name",
        "log_http.status_code",
        "log_usr.name",
        "log_userIdentity.assumed_role",
        "log_usr.id",
        "log_sqreen.payload.rule_id",
        "log_aws.s3.bucket",
        "log_network.client.geoip.country.name",
        "tag_env",
        "log_duration"
    ],
    "page": "stream",
    "type": "logs",
    "options": {
        "stream": {
            "sort": "desc",
            "showDateColumn": true,
            "messageDisplay": "inline",
            "showMessageColumn": true,
            "showTimeline": true,
            "columns": [
                "@env",
                "service",
                "@evt.name",
                "@sqreen.context.http.response.blocked"
            ]
        }
    }
}